@model IEnumerable<onlinecarrer.Models.SeeAppliedJobmodal>

@{
    ViewBag.Title = "AppliedjobbyJs";
    Layout = "~/Views/Shared/_Masterlayout.cshtml";
}

<h2>AppliedjobbyJs</h2>

<h3>YOUR APPLIED JOB DETAILS</h3>
<table class="table table-striped">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.JobId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Companyname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Jobindustry)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Jobkeyskills)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Jobtitle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Jobdescrp)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Appdate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.JobId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Companyname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Jobindustry)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Jobkeyskills)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Jobtitle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Jobdescrp)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Appdate)
        </td>
       
    </tr>
}

</table>
