        <th>
            @Html.DisplayNameFor(model => model.jsPreferredIndustry)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.jsYearOfExperience)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.jsPrevcompName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.jsPreviousTitile)
