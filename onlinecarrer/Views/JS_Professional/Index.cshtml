        <td>
            @Html.DisplayFor(modelItem => item.JsPrevJobDescription)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Job_Seeker.jsFullName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.jsProfID }) |
            @Html.ActionLink("Details", "Details", new { id=item.jsProfID }) |
