            @Html.DisplayFor(modelItem => item.jsPreferredIndustry)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.jsYearOfExperience)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.jsPrevcompName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.jsPreviousTitile)
